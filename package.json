{
  "name": "teledotjs",
  "version": "0.4.2",
  "description": "Telegram MTProto API Client Wrapper Framework in Javascript ",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:docs": "rimraf ./docs && typedoc src/index.ts",
    "clean": "rimraf ./lib ./typings",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telejs/telejs.git"
  },
  "keywords": [
    "telegram",
    "mtproto",
    "api",
    "client",
    "library",
    "framework"
  ],
  "author": "Ahmzan <ahmzan.dev@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/telejs/telejs/issues"
  },
  "homepage": "https://github.com/telejs/telejs#readme",
  "exports": {
    "import": "./src/index.ts",
    "require": "./lib/index.js"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "files": [
    "lib/**/*.js",
    "src/**/*.ts",
    "examples/*.ts",
    "typings/**/*.d.ts",
    "typings/**/*.d.td.map"
  ],
  "types": "./typings/index.d.ts",
  "dependencies": {
    "@mtproto/core": "^5.3.0",
    "dotenv": "^8.2.0",
    "image-size": "^1.0.0",
    "readline-sync": "^1.4.10",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.40",
    "@types/readline-sync": "^1.4.3",
    "@types/sqlite3": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  }
}
